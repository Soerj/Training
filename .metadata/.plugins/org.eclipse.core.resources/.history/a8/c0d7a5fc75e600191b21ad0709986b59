
public class App
{

	public static void main(String[] args)
	{
		Shaman sham;
		Paladin pala;

		sham = new Shaman();
		pala = new Paladin();
		sham.GetClassName();
		pala.GetClassName();
	}

}

class Hero
{
	protected String className;

	public Hero()
	{
		this.className = "Hero";
	}

	public void GetClassName()
	{
		System.out.println(className);
	}

	public void DoJob()
	{
		System.out.println("job's done");
	}
}

class Paladin extends Hero
{
	public Paladin()
	{
		this.className = "Paladin";
	}

	public void DoJob()
	{
		System.out.println("healin!");
	}

	public void Heal()
	{

	}
}

class Shaman extends Hero
{
	public Shaman()
	{
		this.className = "Shaman";
	}

	public void DoJob()
	{
		System.out.println("damagin!");
	}

	public void Damage()
	{

	}
}