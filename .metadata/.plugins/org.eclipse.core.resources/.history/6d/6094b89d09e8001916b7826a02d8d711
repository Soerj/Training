package Core;

class Stats
{
	int health;
	int damage;
	int armour;

	public Stats()
	{
		this.health = 0;
		this.damage = 0;
		this.armour = 0;
	}

	public void Health(int health)
	{
		this.health = health;
	}

	public void Damage(int damage)
	{
		this.damage = damage;
	}
	
	public void Armour(int armour)
	{
		this.armour = armour;
	}

}
